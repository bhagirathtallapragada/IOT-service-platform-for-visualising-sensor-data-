                    <div class="col-lg-8 col-lg-offset-2">
                        <div class="modal-body">
                            <h2>Sensor Data POC</h2>
                            <h4>Please enter the values below for a tentative trial run and POC</h4>
                            <div class="form-inline">
                                <div class="form-group">
                                    <label>Select the SensorID</label>
                                    <select id="SensorId" class="form-control">
                                        <option value="1">1. Level Sensor</option>
                                        <option value="2">2. Flow Sensor</option>
                                    </select>
                                    <label>Enter the value</label><input id="SensorData" class="form-control" type="text" name="SD" /><br />
                                    <button id="btnSave" type="button" class="btn btn-outline">Submit</button>
                                    @*<input id="btnSave"      type="button" class="btn btn-primary" label="post" >Submit</input>*@
                                </div>
                            </div>
                            <div id="SensorDashBoard">
                            </div>
                            <script type="text/javascript">
                                $("#btnSave").click(function () {
                                    SaveSensorData();
                                });
                                function SaveSensorData() {
                                    //alert($("#SensorData").val());
                                    $.ajax({
                                        dataType: "json",
                                        url: "/HOME/UpdateSensorDataDetails",
                                        data: { SensorData: $("#SensorData").val(), SensorId: $("#SensorId").val(), },// parameters being sent to controller
                                        success: function (data) {
                                            getSensorDashReport();
                                        }
                                    });
                                }
                                $("#SensorId").change(function () {
                                    getSensorDashReport();
                                });
                                function getSensorDashReport() {
                                    $.ajax({
                                        dataType: "json",
                                        url: "/HOME/GetSensorDetails",
                                        data: { SensorId: $("#SensorId").val() },
                                        success: function (data) {
                                            if (data.IsSuccess == true) {
                                                if (data.Data != null && data.Data != '') {
                                                    JSON.stringify(data);
                                                    var Data = data.Data; // this comes in the format from the apiresponsemessage class
                                                    var str = [];
                                                    str = '<table class="table table-bordered table-striped" align="center">'
                                                    str += '<tr><th> SensorName </th><th> Data </th><th> GeneratedOn </th></tr>'
                                                    for (i = 0; i < Data.length; i++) {
                                                        if (i == 0) {
                                                            str += '<tr >'
                                                            str += '<td class="table-success">' + Data[i].SensorName + '</td>'
                                                            str += '<td>' + Data[i].Level + '</td>'
                                                            str += '<td>' + Data[i].GeneratedOn + '</td>'
                                                            str += '</tr>'

                                                        }
                                                        else {
                                                            str += '<tr>'
                                                            str += '<td>' + Data[i].SensorName + '</td>'
                                                            str += '<td>' + Data[i].Level + '</td>'
                                                            str += '<td>' + Data[i].GeneratedOn + '</td>'
                                                            str += '</tr>'
                                                        }
                                                    }
                                                    str += '</table>'


                                                    $("#SensorDashBoard").html(str);
                                                }
                                            }
                                        }
                                    });
                                }
                                $(document).ready(function () {
                                    getSensorDashReport();
                                });
                            </script>
                            <button type="button" class="btn btn-default" data-dismiss="modal"><i class="fa fa-times"></i> Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
